    "compilerOptions": {
        "target": "es5",
        "module": "umd",
        "declaration": true,
        "outDir": "dist",
        "strict": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "lib": ["dom", "es2015"]
    }
}
